                                                            <text rel="settings_account_header_account_$" data="$settings_account_header_free">User profile</text>
                                                        </h3>
                                                        <p>
                                                            <text rel="settings_account_pro_description">Account settings</text>
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
